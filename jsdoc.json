{
    "source": {
      "include": ["src"],
      "exclude": ["node_modules", "coverage", "docs"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/minami"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    }
  }
  